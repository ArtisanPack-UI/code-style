{
  "name": "artisanpack-ui/code-style",
  "description": "package name",
  "type": "phpcodesniffer-standard",
  "license": "GPL-3.0-or-later",
  "version": "1.0.1",
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\sniffs\\": "src/ArtisanPackUI/Sniffs/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": ">=5.3",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\CodeStyle\\CodeStyleServiceProvider"
      ],
      "aliases": {
        "Package": "ArtisanPackUI\\CodeStyle\\Facades\\CodeStyle"
      }
    },
    "phpcodesniffer-standard": "ArtisanPackUIStandard"
  }
}
